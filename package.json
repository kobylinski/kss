{
  "name": "niepsuj-kss-template",
  "version": "1.1.1",
  "description": "Niepsuj.it template for kss-node",
  "author": "niespuj.it",
  "license": "MIT",
  "devDependencies": {
    "kss": "^3.0.0-beta.18",
    "node-sass": "^4.6.0"
  },
  "main": "builder.js",
  "dependencies": {
    "highlight.js": "^9.9.0",
    "syntax": "^1.3.4"
  },
  "scripts": {
    "sass": "node-sass --output-style compressed --include-path node_modules/highlight.js/styles/ kss-assets/styles.css kss-assets/styles.css",
    "example": "kss --source ./node_modules/kss/demo --destination docs --builder ./ --custom '<a class=\"niepsuj\" href=\"http://niepsuj.it\"></a>'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/niepsuj/kss.git"
  },
  "homepage": "https://github.com/niepsuj/kss",
  "bugs": "https://github.com/niepsuj/kss/issues",
  "keywords": [
    "kss",
    "kss-node",
    "template"
  ]
}
